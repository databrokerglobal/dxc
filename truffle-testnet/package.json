  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "npx truffle compile && npx typechain --target truffle-v5 './build/contracts/*.json' && npx tsc --declaration",
    "lint": "npx buidler check",
    "test": "npx buidler test --show-stack-traces",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "migrate": "npx truffle compile -all && npx typechain --target truffle-v5 './build/contracts/*.json' && npx tsc --declaration && npx truffle migrate"
  },

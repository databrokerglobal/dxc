    "@types/node": "14.14.33",
    "prettier": "1.19.1",
    "prettier-plugin-solidity": "1.0.0-alpha.49",
    "solc": "0.5.7",
    "solhint": "3.0.0-rc.8",
    "solhint-plugin-prettier": "0.0.4",
    "truffle": "5.1.23",

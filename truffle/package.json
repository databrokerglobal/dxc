  },
  "homepage": "https://databroker.online/",
  "repository": {
    "type": "git",
    "url": "https://github.com/databrokerglobal/dxc.git"
  },
  "dependencies": {
    "@openzeppelin/contracts": "2.5.0",
    "@types/ethereumjs-abi": "^0.6.3",
    "axios": "^0.19.2",
    "bn.js": "^5.1.1",
    "dayjs": "^1.8.26",
    "dotenv": "^8.2.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^4.0.47",
    "faker": "^4.1.0",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "1.3.1",

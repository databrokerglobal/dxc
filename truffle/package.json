    "@nomiclabs/buidler-truffle5": "1.3.4",
    "@nomiclabs/buidler-web3": "1.3.4",
    "@typechain/truffle-v5": "4.0.1",
    "@types/faker": "5.1.6",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.24",
    "prettier": "2.2.1",
    "prettier-plugin-solidity": "1.0.0-beta.3",
    "solc": "0.8.1",
    "solhint": "3.3.2",
    "solhint-plugin-prettier": "0.0.5",
    "truffle": "5.1.64",
    "truffle-privatekey-provider": "1.5.0",

    "url": "https://github.com/databrokerglobal/dxc/issues"
  },
  "homepage": "https://databroker.online/",
  "repository": {
    "type": "git",
    "url": "https://github.com/databrokerglobal/dxc.git"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.4.0",
    "@types/ethereumjs-abi": "^0.6.3",
    "axios": "^0.21.1",
    "bn.js": "^5.1.3",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^5.0.29",
    "faker": "^5.3.1",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "1.4.8",
